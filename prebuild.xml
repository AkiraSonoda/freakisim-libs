<?xml version="1.0" ?>
<Prebuild version="1.10" xmlns="http://dnpb.sourceforge.net/schemas/prebuild-1.10.xsd">
  <Solution activeConfig="Release" name="ArribaSim-Libs" path="./" version="0.5.0-$Rev$">
    <Configuration name="Release">
      <Options>
        <CompilerDefines>TRACE</CompilerDefines>
        <OptimizeCode>true</OptimizeCode>
        <CheckUnderflowOverflow>false</CheckUnderflowOverflow>
        <AllowUnsafe>false</AllowUnsafe>
        <WarningLevel>4</WarningLevel>
        <WarningsAsErrors>false</WarningsAsErrors>
        <SuppressWarnings/>
        <OutputPath>bin</OutputPath>
        <DebugInformation>false</DebugInformation>
        <IncrementalBuild>true</IncrementalBuild>
        <NoStdLib>false</NoStdLib>
      </Options>
    </Configuration>
    <Configuration name="Debug">
      <Options>
        <CompilerDefines>TRACE;DEBUG</CompilerDefines>
        <OptimizeCode>false</OptimizeCode>
        <CheckUnderflowOverflow>false</CheckUnderflowOverflow>
        <AllowUnsafe>false</AllowUnsafe>
        <WarningLevel>4</WarningLevel>
        <WarningsAsErrors>false</WarningsAsErrors>
        <SuppressWarnings/>
        <OutputPath>bin</OutputPath>
        <DebugInformation>true</DebugInformation>
        <IncrementalBuild>true</IncrementalBuild>
        <NoStdLib>false</NoStdLib>
      </Options>
    </Configuration>

    <Project frameworkVersion="v4_0" name="SmartThreadPool" path="CSharp/SmartThreadPool" type="Library">
      <Configuration name="Release">
        <Options>
          <OutputPath>../../bin/</OutputPath>
        </Options>
      </Configuration>
      <Configuration name="Debug">
        <Options>
          <OutputPath>../../bin/</OutputPath>
        </Options>
      </Configuration>

      <ReferencePath>../../bin/</ReferencePath>
      <Reference name="System"/>
      <Reference name="System.Xml"/>
      <Reference name="System.Data"/>
      <Reference name="System.Web"/>
      <Files>
        <Match pattern="*.cs" recurse="true"/>
      </Files>
    </Project>

    <Project frameworkVersion="v4_0" name="C5" path="CSharp/C5" type="Library">
      <Configuration name="Release">
        <Options>
          <OutputPath>../../bin/</OutputPath>
        </Options>
      </Configuration>
      <Configuration name="Debug">
        <Options>
          <OutputPath>../../bin/</OutputPath>
        </Options>
      </Configuration>

      <ReferencePath>../../bin/</ReferencePath>
      <Reference name="System"/>
      <Reference name="System.Core"/>
      <Reference name="System.Xml"/>
      <Files>
        <Match pattern="*.cs" recurse="true"/>
      </Files>
    </Project>

    <Project frameworkVersion="v4_0" name="CookComputing.XmlRpcV2" path="CSharp/XmlRpcNet" type="Library">
      <Configuration name="Release">
        <Options>
          <OutputPath>../../bin/</OutputPath>
        </Options>
      </Configuration>
      <Configuration name="Debug">
        <Options>
          <OutputPath>../../bin/</OutputPath>
        </Options>
      </Configuration>

      <ReferencePath>../../bin/</ReferencePath>
      <Reference name="System"/>
      <Reference name="System.Data"/>
      <Reference name="System.Drawing"/>
      <Reference name="System.Web"/>
      <Reference name="System.Windows.Forms"/>
      <Reference name="System.Xml"/>
      <Files>
        <Match pattern="*.cs" recurse="true"/>
      </Files>
    </Project>

  </Solution>

</Prebuild>
